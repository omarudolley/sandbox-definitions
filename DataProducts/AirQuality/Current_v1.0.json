  "components": {
    "schemas": {
      "BadGateway": {
        "title": "BadGateway",
        "type": "object",
        "properties": {},
        "description": "This response is reserved by Product Gateway."
      },
      "CurrentAirQualityRequest": {
        "title": "CurrentAirQualityRequest",
        "required": ["lat", "lon"],
        "type": "object",
        "properties": {
          "lat": {
            "title": "Latitude",
            "maximum": 90.0,
            "minimum": -90.0,
            "type": "number",
            "description": "The latitude coordinate of the desired location",
            "example": 60.192059
          },
          "lon": {
            "title": "Longitude",
            "maximum": 180.0,
            "minimum": -180.0,
            "type": "number",
            "description": "The longitude coordinate of the desired location",
            "example": 24.945831
          }
        }
      },
      "CurrentAirQualityResponse": {
        "title": "CurrentAirQualityResponse",
        "required": ["airQualityIndex", "timestamp", "attribution"],
        "type": "object",
        "properties": {
          "airQualityIndex": {
            "title": "Air Quality Index",
            "minimum": 0.0,
            "type": "integer",
            "description": "Current air quality index.\nRanges:\n0-50 Good;\n51-100 Moderate;\n101-150 Unhealthy For Sensitive Groups;\n151-200 Unhealthy;\n201-300 Very Unhealthy;\n301+ Hazardous",
            "example": 30
          },
          "timestamp": {
            "title": "Timestamp",
            "type": "string",
            "description": "Current timestamp in RFC 3339 format",
            "example": "2020-04-03T13:00:00Z"
          },
          "attribution": {
            "title": "Source Attribution",
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of text to show required credits to data sources",
            "example": [
              "Eesti välisõhu kvaliteet - Estonian ambient air quality",
              "World Air Quality Index Project"
            ]
          }
        }
      },
      "DataSourceError": {
